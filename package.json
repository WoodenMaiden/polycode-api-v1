{
  "name": "typescript-express-rest-api",
  "version": "2.0.10",
  "main": "./build/index.js",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "node",
    "nodejs",
    "rest",
    "restfull",
    "api",
    "builder",
    "generator"
  ],
  "description": "This generator will help you to build your own Node.js Express Mongodb API using TypeScript.",
  "author": "Checha Valerii <chechavalera@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChechaValerii/node-typescript-mongodb/issues"
  },
  "homepage": "https://github.com/ChechaValerii/node-typescript-mongodb#readme",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./dist/app.js",
    "ts:watch": "tsc -w",
    "test": "echo no tests",
    "start:dev": "npm run build && npx nodemon --config nodemon.json"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "bcrypt": "5.0.1",
    "body-parser": "1.19.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "14.1.0",
    "express": "4.17.2",
    "helmet": "4.6.0",
    "joi": "17.5.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^6.1.6",
    "ts-node": "10.4.0",
    "util": "0.12.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/debug": "4.1.7",
    "@types/dotenv": "8.2.0",
    "@types/ejs": "3.1.0",
    "@types/express": "4.17.13",
    "@types/joi": "17.2.3",
    "@types/jsonwebtoken": "8.5.5",
    "@types/mongoose": "5.11.97",
    "@types/node": "17.0.9",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/yeoman-generator": "5.2.8",
    "chai": "4.3.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jsdoc": "3.6.7",
    "nodemon": "2.0.15",
    "prettier": "^2.3.2",
    "source-map-loader": "3.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "6.2.1",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tslint-eslint-rules": "5.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
